<mui:ModernWindow x:Class="AdminWPFClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        Title="Student tests - Admin" IsTitleVisible="True"
                  
        ContentSource="/Pages/Home.xaml">

    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="Перегляд">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="Панель" Source="/Pages/Home.xaml" />
                <mui:Link DisplayName="Студенти" Source="/Content/StudentInfo.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup DisplayName="Редагування">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="Таблиці" Source="/Pages/EditPage.xaml"/>
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>    
    <mui:ModernWindow.TitleLinks>
        <mui:Link DisplayName="settings" Source="/Pages/SettingsPage.xaml"/>
    </mui:ModernWindow.TitleLinks>
</mui:ModernWindow>
